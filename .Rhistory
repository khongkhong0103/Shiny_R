DataPrep <- function(data, input.gender, input.grade, input.topic) {
if (input.gender != "All") {
data <- filter(data, gender == input.gender)
}
if (input.grade != "All") {
data <- filter(data, GradeID == input.grade)
}
if (input.topic != "All") {
data <- filter(data, Topic == input.topic)
}
return(data)
}
